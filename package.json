{
  "name": "template-react-web",
  "version": "0.1.0",
  "sideEffects": [
    "*.css"
  ],
  "private": true,
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "npm run lint:fix && concurrently -p {name} --names lint,typescript,vite --prefix-colors bgGreen.bold,bgBlue.bold,bgWhite.bold \"npm run lint\" \"npm run type-check\" \"vite --host --port 4000\"",
    "start": "npm run lint:fix && concurrently -p {name} --names lint,typescript,vite --prefix-colors bgGreen.bold,bgBlue.bold,blue.bold \"npm run lint\" \"npm run type-check\" \"npm run dev\"",
    "lint": "biome check ./src && npm run type-check",
    "lint:fix": "biome check --write ./src",
    "type-check": "tsc -b",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "schema": "node scripts/schema-gen.js",
    "test:e2e": "cypress run",
    "bundle-analysis": "BUNDLE_ANALYSIS=true npm run build",
    "cypress:open": "cypress open",
    "prepare": "husky",
    "pre-commit": "concurrently -p {name} --names lint-staged,typescript --prefix-colors bgGreen.bold,bgBlue.bold \"lint-staged --quiet\"",
    "pre-push": "npm run type-check",
    "lighthouse-test:csr": "concurrently --success command-lighthouse --kill-others -p {name} --names firebase-emulator,lighthouse --prefix-colors bgRed.bold,bgBlue.bold \"npm run emulate:csr\" \"wait-on http://127.0.0.1:5002/ && node scripts/lighthouse-test.mjs\"",
    "emulate:csr": "npm run build && firebase emulators:start --only hosting:template-dev",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "dependencies": {
    "@apollo/client": "3.14.0",
    "@coltorapps/builder": "^0.2.4",
    "@coltorapps/builder-react": "^0.2.4",
