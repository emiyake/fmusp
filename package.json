{
  "name": "template-react-web",
  "version": "0.1.0",
  "sideEffects": ["*.css"],
  "private": true,
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "npm run lint:fix && concurrently -p {name} --names lint,typescript,vite --prefix-colors bgGreen.bold,bgBlue.bold,bgWhite.bold \"npm run lint\" \"npm run type-check\" \"vite --host --port 4000\"",
    "start": "npm run lint:fix && concurrently -p {name} --names lint,typescript,vite --prefix-colors bgGreen.bold,bgBlue.bold,blue.bold \"npm run lint\" \"npm run type-check\" \"npm run dev\"",
    "lint": "biome check ./src && npm run type-check",
    "lint:fix": "biome check --write ./src",
    "type-check": "tsc -b",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "schema": "node scripts/schema-gen.js",
    "test:e2e": "cypress run",
    "bundle-analysis": "BUNDLE_ANALYSIS=true npm run build",
    "cypress:open": "cypress open",
    "prepare": "husky",
    "pre-commit": "concurrently -p {name} --names lint-staged,typescript --prefix-colors bgGreen.bold,bgBlue.bold \"lint-staged --quiet\"",
    "pre-push": "npm run type-check",
    "lighthouse-test:csr": "concurrently --success command-lighthouse --kill-others -p {name} --names firebase-emulator,lighthouse --prefix-colors bgRed.bold,bgBlue.bold \"npm run emulate:csr\" \"wait-on http://127.0.0.1:5002/ && node scripts/lighthouse-test.mjs\"",
    "emulate:csr": "npm run build && firebase emulators:start --only hosting:template-dev",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "dependencies": {
    "@apollo/client": "3.13.8",
    "@tailwindcss/vite": "4.1.11",
    "cpf-cnpj-validator": "1.0.3",
    "cross-fetch": "4.1.0",
    "date-fns": "4.1.0",
    "dialog-polyfill": "0.5.6",
    "firebase": "11.8.1",
    "graphql": "16.11.0",
    "graphql-tools": "9.0.18",
    "jose": "6.0.11",
    "lodash.set": "4.3.2",
    "postcss-import": "16.1.0",
    "react": "19.1.0",
    "react-datepicker": "8.4.0",
    "react-dom": "19.1.0",
    "react-error-boundary": "6.0.0",
    "react-hook-form": "7.57.0",
    "react-icons": "5.5.0",
    "react-intersection-observer": "9.16.0",
    "react-masked-text": "1.0.5",
    "react-phone-number-input": "3.4.12",
    "react-router": "7.6.2",
    "react-transition-group": "4.4.5",
    "reflect-metadata": "0.2.2",
    "tailwind-variants": "1.0.0",
    "web-vitals": "5.0.2",
    "zod": "3.25.51"
  },
  "devDependencies": {
    "@babel/preset-env": "7.27.2",
    "@babel/preset-react": "7.27.1",
    "@babel/preset-typescript": "7.27.1",
    "@biomejs/biome": "1.9.4",
    "@graphql-codegen/cli": "5.0.6",
    "@graphql-codegen/fragment-matcher": "5.1.0",
    "@graphql-codegen/named-operations-object": "3.1.1",
    "@graphql-codegen/near-operation-file-preset": "3.0.1",
    "@graphql-codegen/typescript": "4.1.6",
    "@graphql-codegen/typescript-apollo-client-helpers": "3.0.1",
    "@graphql-codegen/typescript-operations": "4.6.1",
    "@graphql-codegen/typescript-react-apollo": "4.3.3",
    "@storybook/addon-actions": "8.6.14",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-interactions": "8.6.14",
    "@storybook/addon-links": "8.6.14",
    "@storybook/addon-onboarding": "8.6.14",
    "@storybook/node-logger": "8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/react-vite": "8.6.14",
    "@storybook/storybook-deployer": "2.8.16",
    "@tailwindcss/postcss": "4.1.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/color": "4.2.0",
    "@types/jest": "29.5.14",
    "@types/lodash.set": "4.3.9",
    "@types/node": "22.15.29",
    "@types/react": "19.1.6",
    "@types/react-dom": "19.1.6",
    "@types/react-transition-group": "4.4.12",
    "@vitejs/plugin-react": "4.5.1",
    "chrome-launcher": "1.2.0",
    "concurrently": "9.1.2",
    "cross-env": "7.0.3",
    "cypress": "14.4.1",
    "dotenv": "16.5.0",
    "firebase-tools": "14.6.0",
    "get-graphql-schema": "2.1.2",
    "graphql-tag": "2.12.6",
    "husky": "9.1.7",
    "imagemin": "9.0.1",
    "imagemin-webp": "8.0.0",
    "lighthouse": "12.6.1",
    "lint-staged": "16.1.0",
    "mini-css-extract-plugin": "2.9.2",
    "react-refresh": "0.17.0",
    "react-refresh-typescript": "2.0.10",
    "storybook": "8.6.14",
    "storybook-addon-apollo-client": "8.1.2",
    "tailwindcss": "4.1.11",
    "tailwindcss-animated": "2.0.0",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vite-plugin-favicons-inject": "2.2.0",
    "vite-plugin-static-copy": "3.0.0",
    "wait-on": "8.0.3"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": ["last 1 chrome version", "last 1 firefox version", "last 1 safari version"]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": ["npx biome check --write"]
  },
  "engines": {
    "node": ">=22.14.0",
    "npm": ">=10.9.2"
  },
  "overrides": {}
}
